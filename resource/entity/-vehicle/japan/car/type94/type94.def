{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "multi_board" "truck" "vision_lev02" "tractor" "tractor_heavy"}
	{able "emit_on_attack"}
	{Extension "type94.mdl"}
	{collider "truck"}
	{patherId "gazaaa"}

	("crew_2_human")
	("board_animation"
		place(driver)
		board_begin(board_big-midcar)
		board_end(seat_driver_car)
		base(doorright)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar_driver)
		board_end(seat_passenger)
		base(doorleft)
	)
	{boarder
		{anm "passenger"
			{forward	{begin	"board_body"} {end "seat_passenger"}}
			{reverse 	{end	"emit_body" 1}}
		}
	}
	("crew_human" place(seat1)	bone(seat01)	door(emit3) animation(passenger))
	("crew_human" place(seat2)	bone(seat02)	door(emit4)	animation(passenger))
	("crew_human" place(seat3)	bone(seat03)	door(emit3)	animation(passenger))
	("crew_human" place(seat4)	bone(seat04)	door(emit4)	animation(passenger))
	("crew_human" place(seat6)	bone(seat06)	door(emit4)	animation(passenger))
	("crew_human" place(seat7)	bone(seat07)	door(emit3)	animation(passenger))
	("crew_human" place(seat8)	bone(seat08)	door(emit4)	animation(passenger))
	("crew_human" place(seat9)	bone(seat09)	door(emit3)	animation(passenger))
	{extender "enumerator"}

		
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")
	("glass" args "glass_front")
	("glass" args "glass_left")
	("glass" args "glass_right")	
	("glass" args "glass_doorleft")
	("glass" args "glass_doorright")
	{volume "shield"
		{thickness 0.01}
		{material "fabric"}
		{able {NoWoundDecal}}
		{component "shield"}
	}
	{component "shield"
		{RepairTime 10}
		{RepairOrder 12}
	}
	{Volume "body"
		{thickness 2}
	}
	{component "body"	
		{RepairTime 10}
	}
	{Volume "cabin"
		{thickness 2}
	}
	{component "cabin"
		{RepairTime 10}
	}
	{Volume "engine"
		{thickness 2}
	}
	{component "engine"	
		{RepairTime 10}
	}
	("bodyback" repair(30))
	{volume "bodyback" {thickness 5} {material "wood"} {tags "wood"} {able {holed}}}

	{sensor
		{visor "driver"
			{vision "tank_driver"}
			{bone "visor1"}
		}
	}
	{extender "shield" {on}}
  	{extender "inventory"
    		{box
		}
	}
	{mass 4600}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(60) ;----------------------------------Max speed.
			speedatmaxturn(17) ;-------------------------Max speed at max turn.
			reverse(12) ;--------------------------------Max speed in Reverse.
			weight(4.60) ;------------------------------Weight in Tonnes.
			power(70) ;----------------------------------Horse Power.
			fuel(100) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(145) ;---------------------------------Range in Kilometers.
		)
	}
	
}