{game_entity	
	(include "/properties/armoredcar.ext")
	(include "/properties/shadow/tank.inc" scale(0.8))
	(include "/properties/selection/vehicle.inc" scale(0.75))
	{props "tank" "turret" "wheeled" "vision_lev02" "tractor" "tractor_light" "detect_tank_mg"}
	{Extension "crossley.mdl"}
	{extension "crew.ext"}
	(include "type87.ext")
	{PatherId "ba64"}
	{collider "truck"}
	{targetclass "armoredcar"}
	{targetSelector "tank_bullet"}	
	{extender "enumerator"
		{digit_folder "jap"}
	}
	{extension "crew.ext"}

	{extender "inventory"
		{weapon "mgun1" {mask "mountable"}}
		{weapon "mgun2" {mask "mountable"}}
		{Box
			{item "ammo 7x58" 5000}
			{item "vickers7_7" filling "ammo 7x58" 251 {user "mgun1"}}
			{item "vickers7_7" filling "ammo 7x58" 252 {user "mgun2"}}
			{item "dynamite"}
		}
	}
	{Weaponry
		{place "mgun1"
			{RestoreIKAfterAim}
			{LinkBone "mgun1"}
			{LinkAnimation "off"}
			{type "mgun_right"}
			{gunner "gunner"}
			{charger "gunner"}
			("abm_mgun")
			{basic}
		}
		{place "mgun2"
			{RestoreIKAfterAim}
			{LinkBone "mgun2"}
			{LinkAnimation "off"}
			{type "mgun_left"}
			{gunner "gunner1"}
			{charger "gunner1"}
			("abm_mgun")
			{basic}
		}

	}  	
	
	
	{mass 7500}
	
	{Chassis "wheel"
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(65) ;----------------------------------Max speed.
			speedatmaxturn(18) ;-------------------------Max speed at max turn.
			reverse(13) ;--------------------------------Max speed in Reverse.
			weight(7.5) ;-------------------------------Weight in Tonnes.
			power(50) ;----------------------------------Horse Power.
			fuel(90) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(350) ;---------------------------------Range in Kilometers.
		)
	}
}
