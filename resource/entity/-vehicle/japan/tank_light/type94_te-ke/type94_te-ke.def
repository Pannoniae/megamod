{game_entity
	(include "/properties/tank.ext")

	(include "/properties/shadow/tank.inc" scale(1.0))

	(include "/properties/selection/vehicle.inc" scale(0.9))
	{props "light" "vision_lev02" "detect_tank_mg"}
	{Extension "tk_type94.mdl"}
	(include "type94_te-ke.ext")
	{targetclass "tank_light"}
	{targetSelector "tank_bullet"}
	{patherId "t27"}
	{collider "tank.light"}
	{extender "enumerator"
		{digit_folder "jap"}
	}

	("crew_2_human")

	{boarder
		{anm "left"
			{forward {begin "board_tank_left"}}
			{reverse {end "emit_tank_1_hold"}}
		}
		{anm "right"
			{forward {begin "board_tank_right"}}
			{reverse {end "emit_tank_1_hold"}}
		}
	}

	{extender "inventory"
		{box
			{item "dynamite"}
			{item "ammo 6x50" 2000}
		}
	}

	{Weaponry
		{place "mgun1"
			{type "main"}
			{weapon "nambu_type91" filled}
			{foresight "foresight4"}
			{gunner "gunner"}
			{charger "gunner"}
			("abm_mgun")
		}
	}

	{mass 3400}
	{Chassis
		("tank_trace" fx("tracks_sm2") step(0.15) len(-0.68))
		("mobility_tank"
			speed(35) ;----------Max speed when fully accelerated on road, km/h.
			reverse(7) ;--------Max speed in reverse on road, km/h.
			traverse(30) ;-------Track traverse, per second, in degrees.
			weight(3.4) ;--------Weight in Tonnes.
			power(32) ;----------Engine Power, in hp.
			track(4.0) ;---------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(100) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(200) ;---------Range in km, off-road.
		)
	}
	

	{bone "turret"
		{speed2 17}
	}
}
