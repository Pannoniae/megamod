{game_entity
	(include "/properties/airborne.ext")
	(include "/properties/selection/airborne.inc" scale(3.0))
	{PatherID "bf110"}
	{props "wheeled" "vision_lev02_air" "a6m21_reisen"}
	("fighter_tier1")
	{boarder
		{anm "driver"
			{forward {begin "board_plane_left"} {end "seat_fi156_driver"} {base "open"}}
			{reverse {end "board_plane_left" -1} {base "open" -1}}
		}
		{anm "gunner"
			{forward {begin "board_plane_left"} {end "seat_fi156_driver"} {base "open2"}}
			{reverse {end "board_plane_left" -1} {base "open2" -1}}
		}
	}

	("crew_human" place(driver) bone(driver) door(emit1) animation(driver) group(crew))
	("crew_human" place(gunner) bone(gunner) door(emit1) animation(gunner) group(crew))

	{Placer
		{place "driver" {visor "main"} {turnOff {targetable}}}
		{place "gunner" {turnOff {targetable}}}
	}

	("single_engine")
	("wheel" args "wheelr")
	("wheel" args "wheell")
	("wheel" args "wheelback")

	
	{Chassis "wheel"
		("car_trace" fx("tracewheel05") step(0.07) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	}

	{Chassis "airborne"
		("flight_model"
			speed(380)  ;//---------Max speed (at sea level), km/h.
			climb_speed(10) ;//-----Climb rate at sea level, in m/s
			turn(180) ;//-----------Turn-and-burn radius in meters, tightest sustained turn
			turnMP(30) ;//----------Turn-and-burn radius in meters, tightest sustained turn in Multiplayer
			aerodynamics(1.2) ;//---Aerodynamic efficiency rating (0-3)
			weight(3800) ;//--------Weight in kg.
			power(1600) ;//---------Horse Power.
			fuel(650) ;//-----------Fuel capacity in Liters.
			range(755) ;//----------Distance can travel with full fuel tank, in km.
			roll_rate(50) ;//-------Roll rate, in degrees per second.
			roll_angle(60) ;//------Maximum roll angle, in degrees.
		)
	}
	
	{mass 3800}
	{extender "inventory"
		{Box
			{item "ammo air_bomb_50kg" 2}
			{item "ammo 7x56" 1000}
		}
	}
	{Weaponry
		{place "mgun1"
		       {weapon "type97mg_x2" filled}
		       {Barrels 2}
               {foresight "foresight4"}
               {shooter "shooter_m"}
		       {type "hull"}
		       {charger "driver"}
		       {gunner "driver"}
		       {rotate}
		       {basic}
        }
		{place "bomb"
			{weapon "bomb_dropper_type94" filled}
			{gunner "driver"}
			{charger "driver"}
			{basic}
		}
	}
	{Extension "d3a_aichi.mdl"}
}
