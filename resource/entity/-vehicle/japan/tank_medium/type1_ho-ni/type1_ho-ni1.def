{game_entity
	(include "/properties/tank_assaultgun.ext")
	(include "/properties/shadow/tank.inc"  scale(1.3))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "td" "medium" "-turret" "mantlet" "vision_lev04" "muzzle_75mm_76mm_sides" "shell_eject" "open_top" "detect_td_medium"}
	{Extension "honi1.mdl"}
	(include "type1_ho-ni1.ext")
	{PatherId "panzer3"}
	{targetclass "tank_medium"}
	{targetSelector "tank_medium"}

	{extender "enumerator"
		{digit_folder "jap"}
	}

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"} {end "pose_vickers_gunlaeyr-L"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"} {end "pose_vickers_gunlaeyr-R"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_MiddleArmor"} {end "pose_seat_armor_1"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_MiddleArmor"} {end "pose_seat_armor_2"}}
			{reverse 	{end	"emit_body"}}
		}
	}

	("crew_human"		place(driver) door1(emit1) door2(emit2) visor(driver))
	("crew_human_visible_anm"	place(commander) door1(emit1) door2(emit2) visor(main) anm_left(left) anm_right(right))
	("crew_human_visible_anm"	place(gunner) door1(emit1) door2(emit2) visor(main) anm_left(left) anm_right(right))
	{placer 
		{place "commander" {openForCrush 0}}
		{place "gunner" {openForCrush 0}}
	}
	("seat_human" place(seat1) bone(seat01) door(emit3)  animation(armor1))
	("seat_human" place(seat2) bone(seat02) door(emit3)  animation(armor))
	("seat_human" place(seat3) bone(seat03) door(emit4)  animation(armor1))
	("seat_human" place(seat4) bone(seat04) door(emit4)  animation(armor))


	{extender "inventory"
		{box
			{item "dynamite"}
			{item "bullet75_jap aphe" 38}
			{item "bullet75_jap he" 5}
			{item "bullet75_jap ic" 2}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "75mm_type90"  filling "ammo bullet75_jap aphe" 1}        
			{charger "commander"}
			("abm_dymamic_td"
				zeroing(2.5)
				dispersion(0.25)
			)
			{rotate}
		}
	}


	{mass 15400}
	{Chassis
		("tank_trace" fx("tracks_mid") step(0.56) len(0.45))
		("mobility_tank"	
			speed(38)  ;---------Max speed when fully accelerated on road, km/h.
			reverse(7.5);----------Max speed in reverse on road, km/h.
			traverse(36) ;-------Track traverse, per second, in degrees.
			weight(15.4) ;-------Weight in Tonnes.
			power(170) ;---------Engine Power, in hp. //engine under normal use was rated 265hp at 2600 rpm
			track(5) ;-----------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(310) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(200) ;----------Range in km, off-road.
		)
	}
	
	{bone "gun_rot"
		{speed2 7}
	}
	{bone "turret"
		{speed2 7}
	}
}
