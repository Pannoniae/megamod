{game_entity
	(include "/properties/tank_assaultgun.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	{props "spg" "light" "-turret" "vision_lev03" "muzzle_50mm_57mm" "detect_spg"}
	{Extension "ft17-3.mdl"}
	(include "ft17.ext")
	{targetclass "tank_light"}
	{targetSelector "tank_light"}
	{patherId "t26"}
	{collider "tank.light"}
	{Volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 16
			{top 8}
		}
	}
	{extender "shield"
		{on}
	}
	{component "shieldl1" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}

	("shield" args "shieldl1" 5)
	("shield" args "shieldr1" 5)

	("ammo" args "ammo1")

	("crew_3_human_2_extended")
	
	{boarder
		{anm "driver"
			{forward	{begin	"board_t34_driver"} {base "doorleft" -1}}
			{reverse 	{end	"emit_t34_hold"} {base "doorleft"  -1}}
		}
		{anm "left"
			{forward	{begin	"board_tank_left"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
	}

	
	{extender "inventory"
		{box
			{item "dynamite"}
			{item "bullet75_fra_241r he" 40}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "75mm_bs" filling "ammo bullet75_fra_241r he" 1}
			{charger "commander"}
			("abm_dymamic"
				zeroing(2.3)
				dispersion(1.0)
			)  
		}
	}
	
	{mass 6700}
	{Chassis
		("tank_trace" fx("tracks_sm2") step(0.15) len(0.45))
		("mobility_tank"
			speed(8) ;----------Max speed when fully accelerated on road, km/h.
			reverse(9) ;--------Max speed in reverse on road, km/h.
			traverse(25) ;-------Track traverse, per second, in degrees.
			weight(6.7) ;--------Weight in Tonnes.
			power(39) ;----------Engine Power, in hp.
			track(7.0) ;---------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(100) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(65) ;----------Range in km, off-road.
		)
	}
	{bone "gun_rot"
		{limits -15 15}
		{speed2 8}
	}
	{bone "gun"
		{limits -15 15}
		{speed2 12}
	}
}