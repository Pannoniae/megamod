{entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "vision_lev01" "tractor"}
	{able "emit_on_attack"}
	{PatherId "kubel"}

	("crew_2_human")
	
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")	
	

	("board_animation"
		place(driver)
		board_begin(board_dodge)
		board_end(seat_passenger)
		base()
	)
	("board_animation"
		place(commander)
	    	board_begin(board_dodge)
    		board_end(seat_passenger)
		base()
	)
	("board_animation"
		place(passenger1)
		board_begin(board_dodge_body)
		board_end(seat_passenger)
		base()
	)

	("crew_human" place(seat1)	bone(seat01)	door(emit3)	animation(passenger1))
	("crew_human" place(seat2)	bone(seat02)	door(emit3)	animation(passenger1))	
	
	{placer
		{place "driver"		{group "gunner"} {visor "driver"}}
		{place "commander"	{group "gunner"} {visor "driver"}}
	}
	{extender "enumerator"
	}
	{extender "inventory"
		{Box
			{size 5 6}
			{item "repair_kit" 2}
		}
	}
	{mass 2600}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(58) ;----------------------------------Max speed.
			speedatmaxturn(10) ;-------------------------Max speed at max turn.
			reverse(18) ;--------------------------------Max speed in Reverse.
			weight(2.6) ;------------------------------Weight in Tonnes.
			power(25) ;----------------------------------Horse Power.
			fuel(60) ;-----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(170) ;---------------------------------Range in Kilometers.
		)
	}	
	
	{Volume "body"
		{tags "body"}
		{component "body"}
		{thickness 7}   
	}
	{Volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 7}   
	}
	{Volume "bodyback"
		{tags "body"}
		{component "body"}
		{thickness 7}   
	}
	{Volume "engine"
		{tags "engine"}
		{component "engine"}
		{thickness 8}  	
	}

	
	{Extension "laffly_v15t.mdl"}
}
