{ArmoredCar
	(include "/properties/armoredcar.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.0))
	{Extension "SdKfz251_17.mdl"}
(include "sdkfz251d_17.ext")
	{props "vehicle" "armored_car" "tank" "turret" "tractor" "zenite" "wheeled_8" "explosion_throw_crew"  "vision_vehicle_medium"  "muzzle_zenite" "switch_mgun"}
              ; {targetSelector "flak"}

        {targetclass "armoredcar"}
	{targetSelector "tank_zenite"}
	
	{volume "body"
		{tags "body"}
		{component "body"}
		{thickness 15}   
	}
	{volume "body2"
		{tags "body"}
		{component "body"}
		{thickness 15}   
	}
	{Volume "bodyl"
		{tags "body"}
		{component "body"}
		{thickness 15}  
	}
	{Volume "bodyr"
		{tags "body"}
		{component "body"}
		{thickness 15}  
	}
	{Volume "engine"
		{tags "engine"}
		{component "engine"}
		{thickness 15}  
	}
	{Volume "gun_rot1"
		{tags "gun"}
		{component "gun"}
		{thickness 10}  	
	}
	{Volume "gun_rot"
		{tags "gun"}
		{component "gun"}
		{thickness 10}
	}
	{Volume "gun"
		{tags "gun"}
		{component "gun"}
		{thickness 10}
	}
	{Volume "mgun_turret"
		{tags "body"}
		{component "body"}
		{thickness 10}
	}
	{Volume "shield"
		{tags "turret"}
		{component "turret"}
		{thickness 20}
	}
	{Volume "stand"
		{tags "body"}
		{component "body"}
		{thickness 10} 
	}
	
	{Volume "turret1"
		{tags "turret"}
		{component "turret"}
		{thickness 10}
	}
	{Volume "turret2"
		{tags "turret"}
		{component "turret"}
