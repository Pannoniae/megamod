	}
	{Volume "turret"
		{tags "turret"}
		{component "turret"}
		{thickness 10}
	}
	{component "body"
		{RepairTime 15}
	}
	{component "engine"
		{RepairTime 20}
	}
	{component "turret"
		{RepairTime 20}
	}
	{component "gun"
		{RepairTime 10}
	}
	 

	("crew_2_human")
	("crew_human" place(gunner) bone(gunner) door(emit1) animation(gunner))
	("crew_human" place(commander) bone(commander) door(emit1) animation(commander))
	("crew_human" place(charger) bone(seat01) door(emit1) animation(charger))

	{sensor
		{visor "main"
			{vision "vehicle_main"}
			{bone "visor"}
		}
		{Visor "driver"
			{vision "tank_driver"}
			{bone "visor2"}
		}
    		{visor "view-around"
        			{vision "none"}
        			{bone "visor2"}
    		}
	}

	{placer
		{place "driver"		{group "crew"}	{visor "driver"}}
		{place "gunner"		{group "crew"}}
		{place "commander"	{group "crew"}}
		{place "charger"	{group "crew"}}
	}	
	{boarder
		{anm "driver"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_driver_car"}  {base "close"}}
			{reverse 	{end	"emit_body" 1}  {base "close" -1}}
		}
		{anm "gunner"
			{forward	{begin	"board_SdKfz251-1"}	{end "pose_flak38-1_gunlayer" 10}  {base "close"}}
			{reverse	{end	"emit_body" 1}  {base "close" -1}}
		}
		{anm "commander"
			{forward	{begin	"board_SdKfz251-1"}	{end "pose_flak38-1_charger" 10}  {base "close"}}
			{reverse 	{end	"emit_body" 7}  {base "close" -1}}
		}
		{anm "charger"
			{forward	{begin	"board_SdKfz251-1"} {end "seat_passenger"}  {base "close"}}
			{reverse 	{end	"emit_body"} {base "close" -1}}
		}
	}
	 
	{extender "enumerator"}

	{extender "inventory"
		{Box
            		{item "bulletger_20_auto ap" 300}		; 600 rds total
			{item "bulletger_20_auto he" 300}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "20mm_kwk38" filling "bulletger_20_auto ap" 10}
			{RestoreIKAfterAim}
			{charger "gunner"}
			("abm_dymamic_autocannon"
				zeroing(2)
				dispersion(0.15)
			)
		}
        }
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	

	{Chassis "wheel"
		("tank_trace_ex" fx("tracks_sm2") step(0.2) len(0.49) boneleft("FXtraceL3") boneright("FXtraceR3"))
		("car_trace" fx("tracewheel03a") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("mobility_car"
			speed(53) ;----------------------------------Max speed.
			speedatmaxturn(14) ;-------------------------Max speed at max turn.
			reverse(9) ;---------------------------------Max speed in Reverse.
			weight(8.8) ;--------------------------------Weight in Tonnes.
			power(100) ;---------------------------------Horse Power.
			fuel(160) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(150) ;---------------------------------Range in Kilometers.
		)
	}
	{mass 8800}
}
