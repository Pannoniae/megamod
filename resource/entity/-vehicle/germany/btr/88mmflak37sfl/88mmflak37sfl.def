	{props "aa" "td" "tank" "shell_eject" "open_top" "turret" "with_gun" "vision_lev02" "muzzle_85mm_93mm" "tractor" "tractor_heavy" "detect_td_medium" "open_death_fx_low"}
	{Extension "88mmflak37sfl.mdl"}
	{volume "body1"
		{thickness 15}
		{component "body"}
		{tags "body"}
	}
	{volume "body2"
		{thickness 15}
		{component "body"}
		{tags "body"}
	}
	{volume "body3"
		{thickness 15}
		{component "body"}
		{tags "body"}
	}
	{volume "body4"
		{thickness 15}
		{component "body"}
		{tags "body"}
	}
	{volume "engine"
		{thickness 10}
		{component "engine"}
		{tags "engine"}
	}
	{volume "inventory"
		{thickness 2}
	}
	{Volume "gun"
		{thickness 10}
		{tags "no_target"}
		{able {Visible 0} {Obstacle 0} {Contact 0}}
	}
	{volume "sticky"
		{tags "grenade_fg_target"}
		{able clear {contact} {touch}}
		{component "sticky_body"}
	}
	{component "sticky_body"}
	{component "shield"
	}
	{extender "shield"
		{on}
	}
	{volume "shield"
		{component "shield"}
		{tags "shield"}
		{thickness 25}
	}
	{extension "crew.ext"}
	{targetclass "armoredcar"}
	{targetSelector "cannon_aa"}
	{patherId "sdkfz8"}

	{sensor
		{visor "main"
			{vision "tank_range_210"}
		}
	}

	{extender "shield" {on}}

	{extender "inventory"
		{box
			{item "bulletger_88_l56 apcbche" 16}	;40 rounds
			{item "bulletger_88_l56 apcr" 5}
			{item "bulletger_88_l56 he" 18}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "88mm_flak18_2" filling "bulletger_88_l56 apcbche" 1}
			{gunner "gunner"}
			{charger "charger"}
			("abm_dymamic_td"
				zeroing(3.5)
				dispersion(0.25)
			)
		}
	}

	{mass 20000}
	{Chassis "wheel"
		("tank_trace" fx("tracks_mid") step(0.5) len(0.59) boneleft("FXtraceL2") boneright("FXtraceR2"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("mobility_car"
			speed(40) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(25) ;--------------------------------Max speed in Reverse.
			weight(20.0) ;-------------------------------Weight in Tonnes.
			power(185) ;---------------------------------Horse Power.
			fuel(250) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(110) ;---------------------------------Range in Kilometers.
		)
	}

	{bone "gun_rot1"		;horizontal gun
		{limits -150 150}	;gun min right to max left
		{speed2 15}
	}
	{bone "gun_rot"			;vertical gun
		{limits -3 85}
		{speed2 15}
	}
}
