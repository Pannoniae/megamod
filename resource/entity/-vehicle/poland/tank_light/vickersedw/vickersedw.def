{game_entity
	(include "/properties/tank.ext")
	(include "/properties/shadow/tank.inc" scale(1.0))
	(include "/properties/selection/vehicle.inc" scale(1.1))
	;{props "light" "vision_lev03" "detect_tank_mg"}
	{props "light" "multi_turret" "vision_lev03" "detect_tank_mg" "t26_31"}
	(include "vickersedw.ext")
	{Extension "vickersedw.mdl"}
	{targetclass "tank_light"}
	{targetSelector "tank_bullet"}
	{patherId "t26"}
	{collider "tank.light"}

	("crew_3_human")

	{boarder
		{anm "left"
			{forward {begin "board_tank_left"}}
			{reverse {end "emit_tank_1_hold"}}
		}
		{anm "right"
			{forward {begin "board_tank_right"}}
			{reverse {end "emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward {begin "board_LowArmor"} {end "seat_armor"}}
			{reverse {end "emit_tank_1"}}
		}
	}

	{sensor
		{visor "main"
			{vision "tank_main-l"}
			{bone "foresight6"}
		}
		{visor "around"
			{vision "tank_main-l"}
			{bone "foresight5"}
		}
	}

	{extender "inventory"
		{box
			{item "dynamite"}
			{item "ammo 7x57b" 6600}		
			{item "satchel_charge_rus" 1}
		}
	}

	{Weaponry
		{place "mgun2"
			{type "mgun_turret_left"}
			{weapon "browning_wz30" filled}
			{foresight "foresight5"}
			{gunner "gunner"}
			{charger "gunner"}
			("abm_mgun")
			{basic}
			;{ai_in_manual}
		}
		{place "mgun3"
			{type "mgun_turret_right"}
			{weapon "browning_wz30" filled}
			{foresight "foresight6"}
			{gunner "charger"}
			{charger "charger"}
			("abm_mgun")
			{basic}
			{ai_in_manual}
		}
	}

	{mass 8000}
	{Chassis
		("tank_trace" fx("tracks_sm2") step(0.2) len(0.37))
		("mobility_tank"
			speed(35) ;----------Max speed when fully accelerated on road, km/h.
			reverse(10) ;--------Max speed in reverse on road, km/h.
			traverse(25) ;-------Track traverse, per second, in degrees.
			weight(8.0) ;--------Weight in Tonnes.
			power(98) ;----------Engine Power, in hp.
			track(7.0) ;---------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(182) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(80) ;----------Range in km, off-road.
		)
	}


	{bone "turret"
		{limits -40 179}
		{speed2 25}
	}
	{bone "aux_turret2"
		{limits -179 50}
		{speed2 25}
	}
}
