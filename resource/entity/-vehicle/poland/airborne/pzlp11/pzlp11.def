{actor
	(include "/properties/airborne_mp.ext")
	(include "/properties/selection/airborne.inc" scale(3.0))
	(include "/properties/car_crew.ext")
    {PatherID "po2"}
	("fighter_tier1")
	{boarder
		{anm "driver"
			{forward	{begin	"board_plane_left"} {end "seat_driver_car"} {base "open"}}
			{reverse 	{end	"board_plane_left" -1} {base "open" -1}}
		}
	}
	("crew_human" place(driver) bone(driver) door(emit1) animation(driver))
	{Placer
		{place "driver" {turnOff {targetable}}}
	}
	("single_engine")

	{Chassis "airborne"
		("flight_model"
			speed(300)  ;//---------Max speed (at sea level), km/h.
			climb_speed(11) ;//-----Climb rate at sea level, in m/s
			turn(200) ;//-----------Turn-and-burn radius in meters, tightest sustained turn
			turnMP(180) ;//----------Turn-and-burn radius in meters, tightest sustained turn in Multiplayer
			aerodynamics(0.30) ;//--Aerodynamic efficiency rating (0-3)
			weight(1800) ;//--------Weight in kg.
			power(640) ;//---------Horse Power.
			fuel(356) ;//----------Fuel capacity in Liters.
			range(700) ;//---------Distance can travel with full fuel tank, in km.
			roll_rate(35) ;//-------Roll rate, in degrees per second.
			roll_angle(60) ;//------Maximum roll angle, in degrees.
		)
	}

	{mass 1800}
	{extender "inventory"
		{box
			{item "ammo 7x57b" 1600}
		}
	}	
	{Weaponry
		{place "zenite"
			{weapon "kmwz33_x4"  filled}
			{barrels 4}
			{foresight "foresight3"}
			{type "main"}
			{charger "driver"}
			{gunner "driver"}
			{rotate}
		}
	}
	{Sensor
		{Visor "main"
			{bone "Foresight3"}
		}
		(mod "mp"
			{Visor "main"
				{vision "airborne_main"}
			}
		)
	}
	{extender "enumerator"}
	{Extension "pzl_p11.mdl"}
	{bone "gun_rot"
		{speed 10}
	}
	{bone "gun_rot1"
		{speed 10}
	}
	
}
