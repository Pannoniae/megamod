{game_entity
    (include "/properties/armoredcar.ext")
    (include "/properties/shadow/tank.inc" scale(1.0))
    (include "/properties/selection/vehicle.inc" scale(1.0))
    {props "tank" "turret" "wheeled" "with_gun" "vision_lev02" "muzzle_25mm" "nwg" "ab40" "armored_command"}
    {Extension "ab40_com.mdl"}
    {extension "crew.ext"}
    {targetclass "armoredcar"}
    {targetSelector "tank_light"}
    {boarder
       {anm "commander"
           {forward     {begin   "board_tank_right"} {end "seat_commander_hatch"}}
           {reverse      {end      "emit_tank_1_hold"}}
       }
    }
    {sensor
       {visor "top"
           {vision "recon_open"}
           {bone "vision_rot"}
       }
    }
    {placer
       {place "commander"
           {group "crew"}
           {linkbone "gunner2"}
           {visor "top"}
           {turnoff {sensor}}
           {openForCrush}
           {sealable}
       }
    }
    ("armoredcar_tier1")
 
    ("turret" repair(50))
    ("gun" repair(30) thickness(20))
 
    ("solid_wheel" args "wheelleft1")
    ("solid_wheel" args "wheelleft2")
    ("solid_wheel" args "wheelright1")
    ("solid_wheel" args "wheelright2")
 
    ("transmission" args "transmission")
    {volume "body1"
       {tags "body"}
       {component "body"}
       {thickness 9
       }
    }
    {volume "body2"
       {tags "body"}
    {component "body"}
       {thickness 9
       }
    }
    {volume "turret"
       {component "turret"}
       {tags "turret"}
       {thickness 9
       {front 18}
       }
    }
      
    {volume "engine"
       {thickness 10
           {rear      15}
       }
    }
    {volume "mantlet"
       {thickness 18}
    }
      
       
    {extender "inventory"
       {box
           {item "ammo 8x59" 3450}
       }
    }
    {Weaponry
       ("restore_ik_time")
       {place "mgun1"
           {RestoreIKAfterAim}
           {type "main"}
           {weapon "breda_38x2" filled}
           {foresight "foresight4"}
		   {gunner "gunner"}
		   {charger "charger"}
           ("abm_mgun")
           {Barrels 2}
           {basic}
       }
       {place "mgun2"
           {type "rear"}
           {weapon "breda_38" filled}
           {foresight "foresight5"}
           {gunner "commander"}
           {charger "commander"}
           ("abm_mgun")
           {basic}
       }
       {place "mgun"
           {type "commander"}
           {weapon "commander_vision"}
           {foresight "vision_rot"}
           {gunner "commander"}
           {basic}
           {ai_in_manual}
       }
    }
 
    {mass 7500}
    {Chassis "wheel"
       ("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
       ("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
       ("mobility_car"
           speed(78) ;----------------------------------Max speed.
           speedatmaxturn(17) ;-------------------------Max speed at max turn.
           reverse(78) ;--------------------------------Max speed in Reverse.
           weight(7.5) ;--------------------------------Weight in Tonnes.
           power(88) ;---------------------------------Horse Power.
           fuel(145) ;----------------------------------Fuel capacity in Litres.
           type(fuel) ;---------------------------------Fuel type.
           range(400) ;---------------------------------Range in Kilometers.
       )
    }
       
 
    {bone "turret"
       {speed2 10}
    }
    {bone "mgun2"
       {speed2 10}
       {limits -10 10}
    }
    {bone "mgun2x"
       {speed2 9}
    }
}