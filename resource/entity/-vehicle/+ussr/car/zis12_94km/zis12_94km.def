{game_entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle_elongated.inc" scale(1.0))
	{props "turret" "with_gun" "open_top" "automatic" "truck" "flak" "vision_lev02" "muzzle_20mm_25mm_auto" "shell_eject"}
	{able "emit_on_attack"}
	{extension "zis12_94km.mdl"}
	{extension "crew.ext"}
	{collider "truck"}
	{patherId "gazaaa"}
	{targetSelector "tank_zenite"}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("glass" args "window5")
	("glass" args "window6")
	("fuel" args "fuel")
	("transmission" args "transmission")

	("bodyback" repair(30))
	("gun" repair(30) thickness(20))
	("turret" repair(45))

	("wood" args "bodyback1")
	("wood" args "bodyback2")
	("wood" args "bodyback3")
	("wood" args "bodyback4")
	("wood" args "bodyback5")

	{volume "fuel" {able {NoWoundDecal 0}}}
	{volume "transmission" {able {NoWoundDecal 0}}}
	{volume "turret1" {thickness 8}}
	{volume "turret2" {thickness 8}}

	{extender "inventory"
		{box
			{item "bulletrus_25_auto apbc" 280}		; 80 clips (7 rounds each)
			{item "bulletrus_25_auto he" 280}
		}
	}

	{Weaponry
		("restore_ik_time")
		{place "gun"
			{weapon "25mm_72k_x2" filling "bulletrus_25_auto he" 14}
			{Barrels 2}
			("abm_dymamic_aaa"
				zeroing(2.5)
				dispersion(0.15)
			)
		}
	}

	{mass 3000}
	{Chassis
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03d") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_car"
			speed(80) ;----------------------------------Max speed.
			speedatmaxturn(14) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(3) ;------------------------------Weight in Tonnes.
			power(73) ;----------------------------------Horse Power.
			fuel(60) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(176) ;---------------------------------Range in Kilometers.
		)
	}

	{bone "gun_rot"
		{limits -8 85}
		{speed2 60}
	}
	{bone "turret"
		{speed2 60}
	}

}
