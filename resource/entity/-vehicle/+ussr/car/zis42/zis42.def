{game_entity
	(include "/properties/car.ext")
	(include "/properties/selection/vehicle_elongated.inc" scale(1.15))
	{props "multi_board" "truck" "halftrack" "wheeled" "explosion_throw_crew" "multi_board" "vision_lev02" "tractor" "tractor_heavy" "Captor"}
	{able "emit_on_attack"}
	{Extension "zis42.mdl"}
	{collider "truck"}
	{patherId "gazaaa"}

	("wheel" args "wheelr1")
	("wheel" args "wheell1")

	("glass" args "window1")
	("glass" args "window2")
	("glass" args "window3")
	("glass" args "window4")
	("glass" args "window5")
	("fuel" args "fuel")
	("transmission" args "transmission")
	("tracks" repair(30) thickness(20))
	("bodyback" repair(30))

	("shield_cover")

	{volume "body7" {material "wood"} {tags "wood"} {able {holed}}}
	{volume "bodyback" {material "wood"} {tags "wood"} {able {holed}}}
	{volume "shield_cover" {able {holed}}}

	("crew_2_human")
	("passenger_15_human")

	("board_animation"
		place(driver)
		board_begin(board_big-midcar_driver)
		board_end(seat_driver_car)
		base(doorleft)
	)
	("board_animation"
		place(commander)
		board_begin(board_big-midcar)
		board_end(seat_passenger)
		base(doorright)
	)

	{boarder
		{anm "passenger"
			{forward {begin "board_body"} {end "seat_passenger"} {base "board"}}
			{reverse {end "emit_body" 1} {base "board"}}
		}
	}

	{extender "inventory"
		{box
		}
	}

	{mass 5268}
	{Chassis
		("tank_trace_ex" fx("tracks_mid") step(0.52) len(-0.7) boneleft("FXtraceL2") boneright("FXtraceR2"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("mobility_car"
			speed(39.8) ;----------------------------------Max speed.
			speedatmaxturn(15) ;-------------------------Max speed at max turn.
			reverse(20) ;--------------------------------Max speed in Reverse.
			weight(5.2) ;--------------------------------Weight in Tonnes.
			power(73) ;----------------------------------Horse Power.
			fuel(180) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(200) ;---------------------------------Range in Kilometers.
		)
	}
}
