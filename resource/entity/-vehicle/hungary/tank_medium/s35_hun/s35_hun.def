{game_entity
	(include "/properties/tank.ext")
	(include "/properties/shadow/tank.inc" scale(1.3))
	(include "/properties/selection/vehicle.inc" scale(1.2))
	{props "medium" "vision_lev03" "muzzle_37mm_47mm"}
	{extension "s35.mdl"}
	(include "s35.ext")
	{targetclass "tank_medium"}
	{targetSelector "tank_medium"}
	{PatherId "t34"}
	("tank_medium_tier2")
	("crew_3_human")
	{boarder
		{anm "driver"
			{forward {begin "board_t34_driver"} {end "seat_gunner_stand"} {base "open_driver"}}
			{reverse {end "emit_t34_hold"} {base "close_driver"}}
		}
		{anm "left"
			{forward {begin "board_Is-2"} {end "seat_commander_hatch"}}
			{reverse {end "emit_tank_1_hold"}}
		}
		{anm "right"
			{forward {begin "board_Is-2"} {end "seat_commander_hatch"}}
			{reverse {end "emit_tank_1_hold"}}
		}
	}

	{extender "shield" {on}}
	
	{extender "inventory"
		{box
			{item "dynamite"}
			{item "ammo 7,5x54" 945}
			{item "bullet47_fra ap" 59}
			{item "bullet47_fra he" 50}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "47mm_sa34" filling "ammo bullet47_fra ap" 1}
			{gunner "gunner"}
			{charger "charger"}
			("abm_dymamic"
				zeroing(3)
				dispersion(0.3)
			)
		}
		{place "mgun2"
			{type "coaxial"}
			{weapon "reibel" filled}
			{foresight "foresight5"}
			{gunner "gunner"}
			{charger "charger"}
			("abm_mgun")
			{basic}
		}
	}
	{mass 19500}
	{Chassis
		("tank_trace" fx("tracks_sm2") step(0.15) len(0.6))
		("mobility_tank"
			speed(40) ;----------Max speed when fully accelerated on road, km/h.
			reverse(6) ;--------Max speed in reverse on road, km/h.
			traverse(37) ;-------Track traverse, per second, in degrees.
			weight(19.5) ;-------Weight in Tonnes.
			power(190) ;---------Engine Power, in hp.
			track(3.4) ;---------Track performance: (0,10], 10=ideal. Sluggish tanks and vehicles with narrow and non-gripping tracks have a lower performance.
			fuel(510) ;----------Fuel capacity in Litres.
			type(fuel) ;---------Fuel type. (diesel OR fuel)
			range(230) ;---------Range in km, off-road.
		)
	}
	{bone "gun_rot"
		{speed2 8}
	}
	{bone "turret"
		{speed2 10}
	}
}
